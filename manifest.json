{
    "name": "TruJS.simpleViewSystem"

    , "config": {
        "strict": true
        , "output": "TruJS/simpleViewSystem"
    }
    , "iocNamePrefix": "ioc-controller"

    , "entries": [
        {
            "type": "ioc.monolith"
            , "base": [
                "{PunyJS.expression}/dtree.json"
            ]
            , "dtree": "dtree.json"
            , "config": {
                "dtreeFileName": "dtree.js"
                , "fileName": "index.js"
                , "engine": "browser"
            }
        }, {
            "type": "test"
            , "units": {
                "controller": 3
                , "app": 0
            }
            , "config": {
                "fileName": "test.json"
            }
        }
        , "{PunyJS.ioc}/manifests/base.manifest.json"
        , "{PunyJS.ioc}/manifests/browser.manifest.json"
    ]
}